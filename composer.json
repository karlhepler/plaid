    "autoload-dev": {
        "classmap": [
            "tests/helpers/ServiceTest.php",
            "tests/helpers/TestCredentials.php",
            "tests/helpers/TestUser.php"
        ],
        "files": [
            "tests/helpers/helpers.php"
